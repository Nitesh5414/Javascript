var start_year = 2000;
var end_year = 2022;
var flag = 0;

console.log("Leap year's between "+start_year+" to "+end_year+" is :<br>");
for(i = start_year; i <= end_year; i++)
{
	if((i%4==0) && (i%100!=0) || (i%400==0))
	{
		flag = 1;
		console.log(i+", ");
	}
	else
	{
		console.log(i+' not a leap year ')
	}

}

if(flag == 0)
console.log("There is no leap year between between the given range");
